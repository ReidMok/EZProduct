{
  "name": "ezproduct",
  "version": "1.0.0",
  "description": "AI-powered Shopify product generator and sync tool - Easy Product Creation",
  "type": "module",
  "scripts": {
    "dev": "shopify app dev --use-localhost",
    "dev:remix": "remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve build/index.js",
    "deploy": "shopify app deploy",
    "generate": "shopify app generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "hostinger:build": "npm install && npm run build && npm run db:generate",
    "hostinger:start": "npm start"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@remix-run/serve": "^2.8.0",
    "@remix-run/dev": "^2.8.0",
    "@shopify/polaris": "^13.0.0",
    "@shopify/shopify-api": "^11.0.0",
    "@shopify/shopify-app-remix": "^2.0.0",
    "@prisma/client": "^5.19.0",
    "isbot": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@shopify/cli": "^3.58.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "prisma": "^5.19.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.0"
  },
  "overrides": {
    "@remix-run/dev": "^2.8.0"
  },
  "engines": {
    "node": "20.x"
  }
}

